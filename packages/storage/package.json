{
  "name": "@tud-box/storage",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./router": {
      "types": "./dist/router.d.ts",
      "default": "./src/router.ts"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "default": "./src/lib/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "storage:start": "docker compose up -d",
    "storage:stop": "docker compose stop",
    "storage:down": "docker compose down"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "dotenv": "catalog:",
    "minio": "^8.0.3",
    "zod": "catalog:",
    "@tud-box/auth": "workspace:*",
    "@tud-box/db": "workspace:*",
    "@better-upload/server": "^3.0.3"
  }
}
